     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
//uniform float[2] fViewportDimensions;
uniform vec2 fViewportDimensions;
uniform float fTime0_X;
void main ()
{
   float zoom = 600;
   float T = mod(fTime0_X,5)/5;
   //    T = 0;
   vec2 center = vec2(fViewportDimensions[0]/2,fViewportDimensions[1]/2);
   //center = vec2(350,200);
   vec2 c = gl_FragCoord.xy;
   //c = (c - center) / (mix(zoom,zoom*4.166666666666667,T)) + vec2(mix(-1,-1.31,sin(T*3.141/2.0)),0);
   c = (c - center) / (mix(zoom,zoom*4.166666666666667,pow(T,1.7))) + vec2(mix(-1,-1.31,T),0);
   vec2 z = c;
   gl_FragColor = vec4(0.2, 0.2, 0.4, 1.0);
   for (float i = 0.0; i < 200.0; i += 1.0) {
      z = vec2(z.x*z.x - z.y*z.y, 2.0*z.x*z.y) + c;
      if (dot(z, z) > 4.0)   {
         gl_FragColor = vec4(i / 200.0, i / 200.0, i / 200.0, 0.0);
         break;
      }
   }
}
]]></RmShaderCode></RmGLShader>
