     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float ftime_0_X;
uniform mat4 matView;

struct Ray {
   vec4 p;
   vec3 v;
};

struct Material {
   vec3 emission;
   vec3 ambient;   
   vec3 diffuse;
   vec3 specular;
   float shininess;
};

struct Sphere {
   vec4 center;
   float radius;
   Material mat;
};

struct Hit {
   vec4 intersection;
   Sphere surface;
   bool isMiss;
};

struct Light {
   vec4 pos;
   vec3 color;
};

struct Camera {
   vec4 pos;   
};

struct Scene {
   vec3 backgroundCol;
   vec3 ambientLight;
   Light lights[1];
   Sphere surfaces[1];
   Camera camera;
};

vec3 Sphere_normalAt(Sphere self, vec4 intersection, in Ray ray) {
   return vec3(normalize(intersection - self.center).xyz);
}

float Sphere_nearestIntersection(in Sphere self, in Ray ray) {

   if (ray.p.x<100)
      return 9999.0;
      
   vec4 v = self.center - ray.p;
   float qB = dot(v.xyz,ray.v);
   float delta = (qB*qB) - pow(length(ray.v),2) * (pow(length(v),2)-pow(self.radius,2));
   
   if (delta <= 0)
      return 9999.0; // No intersection
      
   float sqDelta = sqrt(delta);
   float tp = (qB + sqDelta);
   if(tp < 0)
      return 9999.0;
   float tn = (qB - sqDelta);
   
   return (tn > 0.0) ? tn : tp; // Inside or outside      
}

Hit Scene_findIntersection(Scene self, Ray ray) {   
   
   float minDistance = 9999.0;
   Sphere minSurface;
   for(int i=0;i<self.surfaces.length;++i) {      
      float d = Sphere_nearestIntersection(self.surfaces[i],ray);
      if(d == 9999.0)
         discard;
      if (minDistance > d) {
         minDistance = d;
         minSurface = self.surfaces[i];
      }      
   }
      
   if(minDistance ==  9999.0)
      return Hit(vec4(0), minSurface, true);
   vec4 intersection = ray.p + vec4(minDistance*ray.v,1);
   
   return Hit(intersection, minSurface, false);
}

vec3 Scene_calcColor(Scene self, Hit hit, Ray ray) {
   if(hit.isMiss) 
      return self.backgroundCol;
   Material mat = hit.surface.mat;
   vec3 normal = Sphere_normalAt(hit.surface, hit.intersection, ray);
   
   vec3 res = mat.emission;
   res += self.ambientLight*mat.ambient;
   
   vec3 lightTerm;
   for(int i=0;i<self.lights.length;++i) {
   
      vec3 lightTerm = vec3(0);
      
      vec3 L = normalize(self.lights[i].pos - hit.intersection);
      vec3 R = L;
      vec3 V = normalize(ray.v);
      R = reflect(V,normal);
      
      float da= dot(normal, L);
      if(da < 0)
         continue;
      float sa = dot(R, L);
      if(sa < 0)
         sa = 0;
      lightTerm += (da*mat.diffuse + pow(sa, mat.shininess)*mat.specular)*self.lights[i].color;
      
      res += lightTerm;
   }
   
   return res;
}

Ray constructRayThroughPixel(vec2 p) {
   return Ray(vec4(p, 0,1), vec3(0,0,-1));
}

vec3 castRay(Scene scene, vec2 p) {
   Ray ray = constructRayThroughPixel(p);
   Hit hit = Scene_findIntersection(scene, ray);
   return Scene_calcColor(scene, hit, ray);
}

void main(void)
{
   Material M = Material(
      vec3(0.1,0.1,0.1),
      vec3(0.2,0.2,0.2),
      vec3(0.7,0,0),
      vec3(1,1,1),
      20);
      
   Light[1] L;
   L[0] =  (Light(vec4(10,10,10,1), vec3(1,1,1)));
   Sphere[1] S;
   S[0] = Sphere(vec4(200,200,-200,1),99, M);
   S[0].center  = matView*S[0].center;
   Camera C = Camera(vec4(0,0,0,1));
   Scene scene = Scene(
      vec3(0,0,1), 
      vec3(0.2,0.2,0.2), 
      L, 
      S,
      C);
  
   //S[0].center  = matView*S[0].center;
   //S[0].center  = 0*gl_ModelViewMatrix*S[0].center;
   S[0].center.x = 550;
   
  // L[0].pos = matView*L[0].pos;
  // L[0].pos.z += ftime_0_X;
   gl_FragColor = vec4( castRay(scene, gl_FragCoord.xy), 1.0 );
}]]></RmShaderCode></RmGLShader>
